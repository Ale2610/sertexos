os.pullEvent = os.pullEventRaw

print("Username: ")

username = read()

a = fs.combine(".sertexos/.data", username)
account = fs.open(a, "w")

print("Password: ")

password = read()
account.write(password)
account.close()

eof = true

shell.run("rm", ".sertexos/.data/account")